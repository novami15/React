{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 109",
    "not firefox <= 108",
    "not ios_saf <= 14.8",
    "not opera <= 92",
    "not safari <= 14.1"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color",
          "text-align: -webkit-match-parent"
        ]
      }
    ]
  }
}